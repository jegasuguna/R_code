CacheMatrix <- function(m = matrix()) {
  j <- NULL
  set <- function(y){
  m <<- n
  j <<- NULL
  }
  get <- function()m
  setInverse <- function(inverse) j <<- inverse
  getInverse <- function() j 
  list(set = set, get = get, 
  setInverse = setInverse, 
  getInverse = getInverse)
}

cacheSolve <- function(m, ...) {
  j <- m$getInverse()
  if(!is.null(j)){
  message("getting cached data")
  return(j)
  }
  mat <- m$get()
  j <- solve(mat,...)
  m$setInverse(j)
  j
}
